<% if @organization.present? %>

  <% if @override_non_member_role == 'true' %>
    console.log("lignes deja ($('#non-member-orga-<%@organization.id%>').length): " + $('#non-member-orga-<%@organization.id%>').length);
    if ($('#non-member-orga-<%= @organization.id %>').length==0){
      $('.list.organizations > tbody:first').append("<%= j render :partial => 'projects/settings/non_member_organization', :locals => {:o => @organization} %>");
    }
  <% else %>
    $('.list.organizations > tbody:first').prepend("<%= j render :partial => 'projects/settings/members_organization', :locals => {:o => @organization} %>");
  <% end %>

  hideOnLoad();
  initOrgasSelect2();
  $("#orga-<%= @organization.id %>").effect("highlight");

<% end %>
